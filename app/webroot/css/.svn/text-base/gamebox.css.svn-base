.gamebox{float:left; position:relative; width:223px; height:180px; padding-left:1px; margin:5px;}
.gamebox .greyback{width:210px; height:118px; background-image:url("../img/t_game_box_greyback.png"); padding:5px 6px;}
.gamebox .greyback .whiteback{width:210px; height:118px; background-image:url("../img/t_game_box_whiteback.png");}
.gamebox .greyback .whiteback .gamethumb{margin:4px 5px;}
.gamebox .gb_rate{position:absolute; width:108px; height:36px; background-image:url("../img/t_game_box_rateback.png"); top:110px; left:105px;}
.gamebox .gb_channelname{display:block; text-decoration:none; font-family:Arial; font-size:7pt; color:#004473; margin:5px 3px;}
.gamebox .gb_gamename{width:217px; height:20px; background-image:url("../img/t_game_box_gamenameback.png"); padding-top:14px; padding-left:5px;display:block; text-decoration:none; font-family:Arial; font-size:8pt; color:#004473;}
.gamebox .gb_gamename:hover{background-position:0 -33px;}
.gamebox .gb_rate .rating{width:71px; height:12px; background-image:url("../img/t_ratestars.png"); margin:2px 20px; background-position:-71px 0;}
.gamebox .gb_rate .rating1{width:71px; height:12px; background-image:url("../img/t_ratestars.png"); margin:2px 20px; background-position:-57px 0;}
.gamebox .gb_rate .rating2{width:71px; height:12px; background-image:url("../img/t_ratestars.png"); margin:2px 20px; background-position:-42px 0;}
.gamebox .gb_rate .rating3{width:71px; height:12px; background-image:url("../img/t_ratestars.png"); margin:2px 20px; background-position:-28px 0;}
.gamebox .gb_rate .rating4{width:71px; height:12px; background-image:url("../img/t_ratestars.png"); margin:2px 20px; background-position:-15px 0;}  
.gamebox .gb_rate .rating5{width:71px; height:12px; background-image:url("../img/t_ratestars.png"); margin:2px 20px; background-position:0px 0;}
.gamebox .gb_rate .rateresult{width:84px; font-family:Arial; font-size:10pt; font-weight:bold; color:White; margin-top:3px; margin-left:12px; text-align:center;}